# NetScan Configuration
# Copy this file to .env and update with your values

# Default SSH credentials (optional - can be passed via CLI)
DEFAULT_SSH_USERNAME=admin
DEFAULT_SSH_PASSWORD=
DEFAULT_SSH_PORT=22

# Database configuration
DATABASE_PATH=netscan.db

# Scanning configuration
DEFAULT_THREADS=10
DEFAULT_TIMEOUT=5

# Logging configuration
LOG_LEVEL=INFO
LOG_FILE=netscan.log 